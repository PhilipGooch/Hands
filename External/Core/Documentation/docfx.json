{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Tools/Automation/Automation.csproj",
			"Tools/Automation.Core/Automation.Core.csproj",
			"CoreSample/NBG.*.csproj",
          ],
		  "src": ".."
        }
      ],
      "dest": "api",
	  "globalNamespaceId": "Global",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "filter": "filterconfig.yml"
    }
  ],
  "build": {
	"xrefservice": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
		  "index.md",
		  "toc.yml"
        ]
      },
	  {
        "files": [
		  "Packages/**.md",
		  "Docs/**.md",
          "*.md",
        ],
		"src": ".."
      },
    ],
    "resource": [
      {
        "files": [
		  "Packages/**/Docs~/resources/**",
		  "Docs/resources/**"
        ],
		"src": ".."
      },
	  {
        "files": [
		  "resources/**"
        ]
      },
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../BuildSystem/Artifacts/Documentation",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc",
	  "templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}