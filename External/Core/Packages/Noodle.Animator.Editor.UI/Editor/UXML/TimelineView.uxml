<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="True" name="timelineViewRoot" class="timeline-view-root">
    <Style src="../USS/NoodleAnimatorWindow.uss" />
    <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1">
        <ui:VisualElement name="columnsParent" />
        <ui:VisualElement name="horizontalLinesParent" />
        <ui:VisualElement name="keysParent" usage-hints="GroupTransform" />
        <Noodles.Animation.Editor.UI.CursorView name="editModeCursor" picking-mode="Ignore" usage-hints="DynamicTransform" />
        <Noodles.Animation.Editor.UI.CursorView name="playModeCursor" picking-mode="Ignore" usage-hints="DynamicTransform" />
        <ui:VisualElement name="topBarParent" usage-hints="GroupTransform" style="position: absolute;">
            <Noodles.Animation.Editor.UI.UtilityBarView />
        </ui:VisualElement>
        <ui:VisualElement name="dragSelectorParent">
            <Noodles.Animation.Editor.UI.DragSelectionView name="dragSelectionView" usage-hints="DynamicTransform" />
        </ui:VisualElement>
    </ui:ScrollView>
</ui:UXML>
