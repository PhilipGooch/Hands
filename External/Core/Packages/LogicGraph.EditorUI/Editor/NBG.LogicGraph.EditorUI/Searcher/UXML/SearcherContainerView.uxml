    <ui:VisualElement name="rootContainer" style="padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; flex-grow: 1;">
        <ui:VisualElement name="contentContainer" class="tabs-content-container" style="flex-grow: 1;">
            <ui:VisualElement name="filterFieldContainer" picking-mode="Ignore" style="min-height: 25px; flex-direction: row;">
                <ui:VisualElement name="icon" class="searcher-filter-icon" />
                <ui:TextField picking-mode="Ignore" name="filterField" style="max-height: 20px; flex-grow: 1;" />
            </ui:VisualElement>
            <ui:Label display-tooltip-when-elided="true" name="searcherNotificationView" text="Hidden Nodes Types Count:" style="-unity-font-style: bold; color: rgb(255, 158, 0); font-size: 10px;" />
            <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" name="scrollView" mode="VerticalAndHorizontal" style="flex-grow: 1;" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
