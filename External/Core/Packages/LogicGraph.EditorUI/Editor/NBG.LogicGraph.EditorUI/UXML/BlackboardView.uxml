<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" nbg="NBG.LogicGraph.EditorUI" editor-extension-mode="True" name="inspector" class="inspector-view">
    <Style src="/Packages/com.nobrakesgames.logicgraph.editorui/Editor/NBG.LogicGraph.EditorUI/USS/LogicGraph.uss" />
    <ui:VisualElement name="rootContainer">
        <ui:VisualElement name="headerContainer" class="tabs-header-container" style="margin-bottom: 2px;">
            <ui:Label text="Blackboard" display-tooltip-when-elided="true" name="header" class="tabs-header-label" />
        </ui:VisualElement>
        <ui:VisualElement name="contentContainer" class="tabs-content-container" style="padding-left: 2px; padding-right: 2px; padding-top: 2px; padding-bottom: 2px; flex-shrink: 1;">
            <ui:VisualElement name="variableCreationContainer" style="margin-left: 3px; margin-right: 1px; min-height: 20px;">
                <ui:VisualElement name="addNewVariable" style="align-items: center; flex-direction: row; flex-grow: 1; justify-content: space-between; flex-shrink: 0; min-width: 20px;">
                    <ui:Label text="Add new variable" display-tooltip-when-elided="true" name="addVariableLabel" style="padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;" />
                    <ui:Button text="+" display-tooltip-when-elided="true" name="addVariableButton" style="width: 20px; align-items: stretch; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 1;" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:ScrollView name="variablesContainer" style="margin-left: 3px; margin-right: 1px;" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
