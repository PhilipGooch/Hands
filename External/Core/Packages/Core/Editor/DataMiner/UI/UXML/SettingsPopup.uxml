        <Button text="X" name="closeBtn" style=" right:0; top:0;"/>

        <uie:IntegerField label="Ms line height" name="msLineHeight"/>
        <uie:IntegerField label="Ms cap timings graph" name="msCap"/>
        <SliderInt label="Graphs height"  name="graphsHeighSelection" low-value="50" high-value="300"/>
        <SliderInt label="Column width"  name="columnWidthSelection" low-value="1" high-value="20"/>

     </Box>
</UXML>
