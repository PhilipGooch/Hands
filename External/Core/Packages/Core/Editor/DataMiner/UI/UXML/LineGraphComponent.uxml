<UXML xmlns="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">  
    <Box name="countersGraph" class="component">
        <Box class="horizontal">
            <Label name="sliceDetails" text="Counters"/>
              <Button name="delete" class="button" text="X" style="right:0; position:absolute;"/>
        </Box>
        <Box name="graphBox" class="component-graph graph-bg">
            <VisualElement name="selectionColumns" class="graph-plot-elements-parent" style="right:0; position:absolute;"/>
            <VisualElement name="gridLines" class="graph-plot-elements-parent" style="right:0; position:absolute;"  picking-mode="Ignore"/>
            <VisualElement name="graphLines" class="graph-plot-elements-parent" style="right:0; position:absolute;"  picking-mode="Ignore"/>

        </Box>
        <Box name="toggleDatasets" class="horizontal" style="flex-wrap: wrap;"/>

        <Box name="graphDetailsBox" />
    </Box>
</UXML>